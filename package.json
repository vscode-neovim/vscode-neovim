				"key": "ctrl-r shift-'",
				"when": "editorTextFocus && neovim.mode == insert && neovim.ctrlKeysInsert",
				"args": "\""
			},
			{
				"command": "vscode-neovim.paste-register",
				"key": "ctrl-r shift-5",
				"when": "editorTextFocus && neovim.mode == insert && neovim.ctrlKeysInsert",
				"args": "%"
			},
			{
				"command": "vscode-neovim.paste-register",
				"key": "ctrl-r shift-3",
				"when": "editorTextFocus && neovim.mode == insert && neovim.ctrlKeysInsert",
				"args": "#"
			},
			{
				"command": "vscode-neovim.paste-register",
				"key": "ctrl-r shift-8",
				"when": "editorTextFocus && neovim.mode == insert && neovim.ctrlKeysInsert",
				"args": "*"
			},
